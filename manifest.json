{
    "manifest_version": 2,
    "name": "TFRRS Performance List Filter",
    "description": "Highlights the performances of athletes in the MIAC.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["*://*.tfrrs.org/lists/*"],
            "js": [
                "jquery-1.11.0.min.js",
                "config.js",
                "background.js"
            ]
        }
    ]
}